# Environment Configuration Example
# Copy this file to .env and fill in your actual values

# Expo Configuration
EXPO_PUBLIC_ENVIRONMENT=development

# API Keys (These are provided in the Vibecode environment)
EXPO_PUBLIC_VIBECODE_OPENAI_API_KEY=your_openai_api_key_here
EXPO_PUBLIC_VIBECODE_ANTHROPIC_API_KEY=your_anthropic_api_key_here
EXPO_PUBLIC_VIBECODE_GROK_API_KEY=your_grok_api_key_here

# App Configuration
EXPO_PUBLIC_APP_NAME=Phi-4 Reasoning
EXPO_PUBLIC_APP_VERSION=1.0.0

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=false
EXPO_PUBLIC_DEBUG_MODE=true

# Performance Settings
EXPO_PUBLIC_MAX_TOKENS=512
EXPO_PUBLIC_CONTEXT_LENGTH=128000
EXPO_PUBLIC_QUANTIZATION=INT4